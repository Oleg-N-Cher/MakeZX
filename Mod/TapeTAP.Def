PROCEDURE (VAR tap: TapeFile) SaveCode (
  name: ARRAY OF CHAR; startAddr, dataLength: INTEGER; VAR data: ARRAY OF BYTE);
PROCEDURE (VAR tap: TapeFile) Finalize;

END TapeTAP.
