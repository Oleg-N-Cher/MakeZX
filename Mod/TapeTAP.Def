DEFINITION TapeTAP;

IMPORT
  Platform, Files;

TYPE
  TapeFile = RECORD
    error-: BOOLEAN;
  END;

PROCEDURE (VAR tap: TapeFile) ReCreate (tapeName: STRING);
PROCEDURE (VAR tap: TapeFile) SaveCode (
  name: STRING; startAddr, dataLength: INTEGER; VAR data: ARRAY OF BYTE);
PROCEDURE (VAR tap: TapeFile) Finalize;

END TapeTAP.
